{
  "name": "linq-ds/tailwind",
  "version": "0.1.0",
  "description": "LINQs official design tokens, Tailwind preset, and branded assets.",
  "license": "MIT",
  "repository": "https://github.com/blduffie/linq-ds.git",
  "author": "LINQ",
  "contributors": [
    "Bronwyn Duffield <bronwyn.duffield@linq.com>"
  ],
  "main": "./index.js",
  "peerDependencies": {
    "tailwindcss": "^3.4.4"
  },
  "devDependencies": {
    "@11ty/eleventy": "^1.0.0",
    "autoprefixer": "^10.4.13",
    "postcss": "^8.4.20",
    "tailwindcss": "^3.4.4"
  },
  "dependencies": {
    "@tailwindcss/container-queries": "^0.1.1"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "copy:base": "cp -r ./src/* ./dist",
    "copy:root": "cp package.json ./dist && cp README.md ./dist",
    "build": "npm run clean && mkdir dist && npm run copy:root && npm run copy:base",
    "dev": "tailwindcss -i src/input.css -o dist/output.css --watch",
    "publish": "npm run build && npm publish"
  }
}
